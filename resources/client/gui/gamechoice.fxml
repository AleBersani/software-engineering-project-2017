      <JFXButton fx:id="resumeGame" buttonType="RAISED" layoutX="84.0" layoutY="206.0" text="Resume game"
                 styleClass="button" onAction="#resumeGame" focusTraversable="false"/>
   </children>
</AnchorPane>
